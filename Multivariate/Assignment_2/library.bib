doi = {10.1017/S0022381608080316},
file = {:C$\backslash$:/Users/gwehrm/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Giles, Blackstone, Vining - 2008 - The Supreme Court in American Democracy Unraveling the Linkages between Public Opinion and Judicial D.pdf:pdf},
isbn = {0022-3816},
issn = {0022-3816},
journal = {The Journal of Politics},
