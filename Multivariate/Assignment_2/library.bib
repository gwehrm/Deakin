@article{Everson1981,
abstract = {Voter turnout in national elections continues to decline. Recent efforts to adopt a national initiative have raised the question of whether initiatives increase voter turnout on the assumption that specific issues attract voters to the polls. Previous research is inconclusive on the question. This study examines turnout in states which held initiatives versus states without initiatives in the years 1960-78. Examining all states, there is a modest turnout advantage for initiative states in both presidential and off-years. With the Southern states removed, however, that advantage is eliminated. It thus seems likely that other factors, such as political culture, account for the differences between initiative and non-initiative states. The conclusion is that while initiatives may have a positive impact on turnout in special circumstances, no general long-term positive impact is likely. Over a series of elections, a national initiative held at the time of general elections would not be expected to affect turnout significantly.},
annote = {Says that there are two different ways how initiatives work. One is that salient issues do influence the turnout in general elections.},
author = {Everson, David H},
